rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{userId}/{allPaths=**} {
      

      
      // READ Rules: Publicly accessible (relying on token or just public)
      allow read: if true;
      
      // WRITE Rules
      allow write: if request.auth != null && (request.auth.uid == userId || request.auth.token.admin == true);
    }
  }
}
